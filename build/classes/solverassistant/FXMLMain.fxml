<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>

<AnchorPane id="AnchorPane" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" styleClass="title-backgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="solverassistant.FXMLMainController">
    <children>
        <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
            <tabs>
                <Tab id="loadTab" fx:id="loadTab" text="Load Log">
                    <content>
                        <fx:include fx:id="barTabPageLoad" source="FXMLLoadLog.fxml" />
                    </content>
                </Tab>
                <Tab id="editTab" fx:id="editTab" text="Edit Solver" disable="true" >
                    <content>
                        <fx:include fx:id="barTabPageEdit" source="FXMLEditSolver.fxml" />
                    </content>
                </Tab>
                <Tab id="compareTab" fx:id="compareTab" text="Compare from database">
                    <content>
                        <fx:include fx:id="barTabPageCompare" source="FXMLCompare.fxml" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <ComboBox id="comboLanguage" fx:id="comboLanguage" layoutX="1111.0" layoutY="14.0" prefHeight="25.0" prefWidth="100.0" promptText="Language" styleClass="comboBox-defaultComboBox" visibleRowCount="3" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0" />
    </children>
    <stylesheets>
        <URL value="@/style/style.css" />
    </stylesheets>
</AnchorPane>
